            });
            $("#btnUpdate").on("click", function () {
                if (!$("#acceptRules").is(':checked')) {

                    return;
                }
                var brth = $("#birthDate").val();
                var valid = true;
                if (brth === null || brth === '' || brth === undefined) {
                    $("#birthDate").addClass("error");
                    valid = false;
                }

                if (valid) {

                    var subscriber = {
                        birthDate: brth,
                        sex: $("#sex").val(),
                        name: $("#txtname").val(),
