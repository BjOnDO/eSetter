                                timer = setTimeout(function () {
                                    $('[data-toggle="popoverSignIn"]').popover('hide');
                                    timer = null;
                                    $(".registriraj").css("visibility", "");
                                    $(".popover").css("z-index", "-9999");
                                }, 3500);
                            }
                            if (xhr.status == 406) {
                                $("#exampleModal").modal('show');
                            }
                        });
                }
            });
            $(".condition").on("click", function () {
                $("#policyModal").modal({
                    keyboard: false,
                    backdrop: "static"
                });
            });
            $(".closeMdl").on("click", function () {
                if (timer != null) {
                    clearTimeout(timer);
                    $('[data-toggle="popoverSignIn"]').popover('hide');
                    $('[data-toggle="popoverNewPass"]').popover('hide');
                    $('[data-toggle="popoverCngPass"]').popover('hide');
                    timer = null;
                }
                $(".popover .row").removeClass("errorText").removeClass("validatedText").addClass("errorText");
                $("#userMail").removeClass("error");
                $("#userMail").val('');
                $("#prefixReset").val('070');
                $("#birthDateReset").removeClass("error");
                $("#birthDateReset").val('');
                $("#birthDate").removeClass("error");
                $("#birthDate").val('');
                $("#sex").val('');
                $("#txtname").val('');
                $("#txtlastName").val('');
                $("#repeatNewPass").removeClass("error").removeClass("validated");
                $("#repeatNewPass").val('');
                $("#newPass").removeClass("error");
                $("#newPass").val('');
