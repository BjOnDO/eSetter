                var isValid = false;
                var clLength = 'errorText';
                var clUpper = 'errorText';
                var clSpec = 'errorText';
                if (val.length >= 8) {
                    clLength = "validatedText";
                } else {
                    clLength = "errorText";
                }
                if (/^[a-zA-Z0-9- ]*$/.test(val) == false) {
                    clSpec = "validatedText";
