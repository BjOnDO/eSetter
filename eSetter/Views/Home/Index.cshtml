                    $("#username").addClass("error");
                    valid = false;
                }
                if (oldpass === null || oldpass === '' || oldpass === undefined) {
                    $("#oldPass").addClass("error");
                    valid = false;
                }
                if (newpas === null || newpas === '' || newpas === undefined) {
                    $("#newPass").addClass("error");
                    valid = false;
                }
                if (repeatNewPass === null || repeatNewPass === '' || repeatNewPass === undefined || repeatNewPass !== newpas) {
                    $("#repeatNewPass").addClass("error");
                    valid = false;
                }
                if (valid) {
                    $("#cngPass").css("visibility", "hidden");
                    $("#clsCngPas").css("visibility", "hidden");
                    $(".cngPasCls").addClass("loader");
                    var subscriber = {
