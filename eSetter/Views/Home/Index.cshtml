                var clSpec = 'errorText';
                var isValid = false;
                if (val.length >= 8) {
                    clLength = "validatedText";
                }
                if (/^[a-zA-Z0-9- ]*$/.test(val) == false) {
                    clSpec = "validatedText";
                }
                for (i = 0; i < val.length; i++) {
                    if (isItUpper(val.charAt(i))) {
                        isValid = true;
                    }
                }
                if (isValid) {
                    clUpper = "validatedText";
                }
                var popover = $('[data-toggle="popoverNewPass"]').data('bs.popover');
                if (!popover.tip().is(':visible')) {
                    $('[data-toggle="popoverNewPass"]').popover('show');
                }
