                if (keycode == '13') {
                    $("#signIn").trigger('click');
                }
            });
            var i = 0;
            var m = 0;
            var txt = '149444';
            var txt2 = 'START';
            var l = txt2.length;
            var p = txt.length;
            var speed = 50;
            var equal = true;
            typeWriter();
            var data;

            function typeWriter() {
                if (i < txt.length) {
                    document.getElementById("target").innerHTML += '|';
                    var el = document.getElementById("target").innerHTML;
                    setTimeout(function () {
                        document.getElementById("target").innerHTML = el.substring(0, el.length - 1);
                        document.getElementById("target").innerHTML += txt.charAt(i);
                        i++;
                        setTimeout(typeWriter, speed);
                    }, 150);

                } else {
                    if (m < txt2.length) {
                        document.getElementById("target2").innerHTML += '|';
                        var el = document.getElementById("target2").innerHTML;
                        setTimeout(function () {
                            document.getElementById("target2").innerHTML = el.substring(0, el.length - 1);
                            document.getElementById("target2").innerHTML += txt2.charAt(m);
                            m++;
                            setTimeout(typeWriter, speed);
                        }, 150);
                    } else {
                        if (l === txt2.length && equal) {
                            document.getElementById("target2").innerHTML += '|';
                            equal = false;
                            var myVar = setInterval(function () {
                                document.getElementById("target2").innerHTML.substring(l, l + 1) === '|' ? document.getElementById("target2").innerHTML = document.getElementById("target2").innerHTML.substring(0, l) + ' ' : document.getElementById("target2").innerHTML = document.getElementById("target2").innerHTML = document.getElementById("target2").innerHTML.substring(0, l) + '|'
                            }, 130);
                            setTimeout(function () {
                                clearInterval(myVar);
                                document.getElementById("target2").innerHTML = document.getElementById("target2").innerHTML.substring(0, l - 1);
                                typeWriter();
                            }, 2000);
                        } else {
                            if (l > 0) {
                                document.getElementById("target2").innerHTML += '|';
                                var el = document.getElementById("target2").innerHTML;
                                setTimeout(function () {
                                    document.getElementById("target2").innerHTML = el.substring(0, l - 2);
                                    l--;
                                    setTimeout(typeWriter, speed);
                                }, 150);
                            } else {
                                if (p > 0) {
                                    document.getElementById("target").innerHTML += '|';
                                    var el = document.getElementById("target").innerHTML;
                                    setTimeout(function () {
                                        document.getElementById("target").innerHTML = el.substring(0, p - 2);
                                        p--;
                                        setTimeout(typeWriter, speed);
                                    }, 150);
                                } else {
                                    i = 0;
                                    m = 0;
                                    l = txt2.length;
                                    p = txt.length;
                                    equal = true;
                                    setTimeout(typeWriter, speed);
                                }
                            }
                        }
                    }
                }
            }
            //$.ajax({
