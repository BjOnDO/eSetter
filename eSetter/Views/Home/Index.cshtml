                $("#passLength").removeClass("errorText").removeClass("validatedText").addClass(clLength);
                $("#passUpper").removeClass("errorText").removeClass("validatedText").addClass(clUpper);
                $("#passSpec").removeClass("errorText").removeClass("validatedText").addClass(clSpec);
                $('.popover').css("z-index", "1001");

            });
            $('#newPass').keyup(function () {
