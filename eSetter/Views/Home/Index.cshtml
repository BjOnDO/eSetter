            }).on('changeDate', function () {
                $(this).datepicker('hide');
            });
            $("#birthDate").datepicker({
                language: 'mk',
                startDate: '01/01/1920',
                endDate: dt
            }).on('changeDate', function () {
                $(this).datepicker('hide');
            });
            $('#birthDateReset').keypress(function (e) {
                e.preventDefault();
            });
            $('#birthDate').keypress(function (e) {
                e.preventDefault();
            });

            $("input").focus(function () {
                $(this).removeClass("error");
            });
            $('#changePass').on('shown.bs.modal', function () {
                $('#username').trigger('focus')
            });
            $('#resetPass').on('shown.bs.modal', function () {
                $('#userMail').trigger('focus')
            });

            function getCookie(cname) {
                var name = cname + "=";
                var decodedCookie = decodeURIComponent(document.cookie);
                var ca = decodedCookie.split(';');
                for (var i = 0; i < ca.length; i++) {
                    var c = ca[i];
                    while (c.charAt(0) == ' ') {
                        c = c.substring(1);
                    }
                    if (c.indexOf(name) == 0) {
                        return c.substring(name.length, c.length);
                    }
                }
                return "";
            }
            $("#signIn").on("click", function () {
                $(".popover").css("z-index", "");
                if (timer != null) {
                    clearTimeout(timer);
                    $('[data-toggle="popoverSignIn"]').popover('hide');
                    timer = null;
                }
                var usr = $("#user").val();
                var pas = $("#pass").val();
                var valid = true;
                if (usr === null || usr === '' || usr === undefined) {
                    $("#user").addClass("error");
                    valid = false;
                }
                if (pas === null || pas === '' || pas === undefined) {
                    $("#pass").addClass("error");
                    valid = false;
                }

                if (valid) {
                    $(this).css("visibility", "hidden");
                    $(".signInLd").addClass("loaderLogin");
                    var subscriber = {
                        username: usr,
                        password: pas,
                        prefix: $("#prefix").val()
                    };
                    $.post('../api/eSetter/checkAccount', subscriber)
                        .done(function (data) {
                            $('#formDev').html(data.data);

                            document.forms['redirectForm'].submit();
                        })
                        .fail(function (xhr, status, error) {

                            $(".signInLd").removeClass("loaderLogin");
                            $("#signIn").css("visibility", "");
                            if (xhr.status == 404) {
                                $("#pass").addClass("error");
                                $("#user").addClass("error");
                                $(".registriraj").css("visibility", "hidden");
                                $('[data-toggle="popoverSignIn"]').popover('toggle');
